<%- include("partials/header"); %>
<h1>Compose</h1>

<form action="/compose" method="post">
	<div class="form-group">
		<label for="newPostTitle">Title</label><br />
		<input
			type="text"
			name="newPostTitle"
			autocomplete="off"
			class="form-control"
		/><br />

		<label for="newPost">Post</label><br />
		<textarea
			name="newPost"
			id=""
			cols="30"
			rows="10"
			class="form-control"
		></textarea
		><br />
	</div>
	<button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer"); %>
